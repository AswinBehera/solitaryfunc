{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "strictNullChecks": true
  },
  "paths": {
    "@/assets/*": [
      "src/assets/*"
    ],
    "@/components/*": [
      "src/components/*"
    ],
    "@/layouts/*": [
      "src/layouts/*"
    ],
    "@/icons/*": [
      "src/icons/*"
    ],
    "@/i18n/*": [
      "src/i18n/*"
    ],
    "@/data/*": [
      "src/data/*"
    ]
  }
}
